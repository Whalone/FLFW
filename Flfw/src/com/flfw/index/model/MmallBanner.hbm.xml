<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.flfw.index.model.MmallBanner" table="mmall_banner" schema="mmall">
        <id name="id">
            <column name="id" sql-type="int(255)"/>
        </id>
        <property name="imgSrc">
            <column name="img_src" sql-type="varchar(255)"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(6)"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="updateTime">
            <column name="update_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>